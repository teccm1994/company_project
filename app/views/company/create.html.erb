<div style="margin:100px auto;width:40%;border:#e8e8e8 2px solid">
	<label style="font-size:16px;margin-left:49px;margin-top:10px">新增部门</label>
	<form action="/company/add" method="post">
	  <div class="form-group" style="margin:0 auto;width:80%">
	    <label for="forusername">部门名称:</label>
	    <input type="text" name="name" class="form-control" id="forusername" placeholder="部门名称">
	  </div>
	  <input type="hidden" name="pre_id" value="<%=@pre_id%>"/>
	  <button style="margin-left:49px;width:100px;margin-top:15px;margin-bottom:10px" type="submit" class="btn btn-default">提交</button>
	</form>
</div>